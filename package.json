{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chatgpt",
  "title": "ChatGPT",
  "description": "Prompt ChatGPT easily",
  "icon": "command-icon.png",
  "author": "domnantas",
  "categories": [
    "Fun",
    "Other",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "ChatGPT",
      "description": "Prompt ChatGPT",
      "mode": "view",
      "preferences": [
        {
          "name": "chatGPTToken",
          "title": "ChatGPT JWT token",
          "description": "Click About This Extension to learn how to obtain it",
          "type": "password",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "stream-json": "^1.7.5"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
